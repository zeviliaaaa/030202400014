#include <iostream>
using namespace std;

    int main()    xge9ouio
    
    // Declare and initialize two 3x3 matrices
    int matrix1[3][3] = {
        {1, 2, 3},
        {4, 5, 6},
        {7, 8, 9}
    };

    int matrix2[3][3] = {
        {10, 11, 12},
        {13, 14, 15},
        {16, 17, 18}
    };

    int matrix3[3][3] = {
        {19, 20, 21},
        {22, 23, 24},
        {25, 26, 27}
    };

    // Display the first matrix
    cout << "Matrix 1:" << endl;
    cout << matrix1[0][0] << " " << matrix1[0][1] << " " << matrix1[0][2] << endl;
    cout << matrix1[1][0] << " " << matrix1[1][1] << " " << matrix1[1][2] << endl;
    cout << matrix1[2][0] << " " << matrix1[2][1] << " " << matrix1[2][2] << endl;

    // Display the second matrix
    cout << "\nMatrix 2:" << endl;
    cout << matrix2[0][0] << " " << matrix2[0][1] << " " << matrix2[0][2] << endl;
    cout << matrix2[1][0] << " " << matrix2[1][1] << " " << matrix2[1][2] << endl;
    cout << matrix2[2][0] << " " << matrix2[2][1] << " " << matrix2[2][2] << endl;

    // Display the third matrix
    cout << "\nMatrix 3:" << endl;
    cout << matrix3[0][0] << " " << matrix3[0][1] << " " << matrix3[0][2] << endl;
    cout << matrix3[1][0] << " " << matrix3[1][1] << " " << matrix3[1][2] << endl;
    cout << matrix3[2][0] << " " << matrix3[2][1] << " " << matrix3[2][2] << endl;

    return 0;
}
